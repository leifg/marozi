.navbar-wrapper
  .container
    .navbar.navbar-inverse
      .navbar-inner
        %a.btb.btn-navbar.collapsed{:'data-toggle' => 'collapse', :'data-target' => '.nav-collapse'}
          %span.icon-bar
          %span.icon-bar
          %span.icon-bar
        %a.brand{href: '#'} Marozi
        .nav-collapse.collapse{style: 'height:auto;'}
          %ul.nav
            %li.dropdown
              %a{href: '#', class: 'dropdown-toggle', :'data-toggle' => 'dropdown'}
                = t(:'navbar.profile.title')
                %b.caret
              %ul.dropdown-menu
                %li
                  %a{href: my_membership_path}= t(:'navbar.profile.membership')
                %li
                  %a{href: my_club_path}= t(:'navbar.profile.my_club')
                %li
                  %a{href: my_district_path}= t(:'navbar.profile.my_district')
                %li
                  %a{href: my_multiple_district_path}= t(:'navbar.profile.my_multiple_district')
            %li.dropdown
              %a{href: '#', class: 'dropdown-toggle', :'data-toggle' => 'dropdown'}
                = t(:'navbar.club.title')
                %b.caret
              %ul.dropdown-menu
                %li
                  %a{href: my_club_path}= t(:'navbar.club.overview')
                %li
                  %a{href: '#'}= t(:'navbar.club.edit_club')
                %li
                  %a{href: '#'}= t(:'navbar.club.add_member')
                %li
                  %a{href: '#'}= t(:'navbar.club.add_guest')
                %li
                  %a{href: '#'}= t(:'navbar.club.accept_to_club')