.cotainer
  .page-header
    %h1= t(:'clubs.caption.clubs_in_md', md: '111 Germany')

  .row
    .span3.navbar
      %ul.nav.nav-list.navbar.affix-top
        %li bla

    .span9
      - @clubs.each do |district_name, clubs|
        %section{id: district_name}
          %h2= t(:'districts.district_title',multiple_district: '111', district: district_name)
          = render 'shared/clubs_list_table', clubs: clubs